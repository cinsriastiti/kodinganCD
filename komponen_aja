//SERVO AJA
#include <Servo.h>
Servo servo1; // Servo pertama
Servo servo2; // Servo kedua

void setup() {
  servo1.attach(9);  // pin sinyal servo1
  servo2.attach(10); // pin sinyal servo2
  Serial.begin(9600);
}

void loop() {
  // Gerakan 0 derajat
  servo1.write(0);
  servo2.write(0);
  Serial.println("Servo 1 dan 2 ke 0 derajat");
  delay(1000);

  // Gerakan 90 derajat
  servo1.write(90);
  servo2.write(90);
  Serial.println("Servo 1 dan 2 ke 90 derajat");
  delay(1000);

  // Gerakan 180 derajat
  servo1.write(180);
  servo2.write(180);
  Serial.println("Servo 1 dan 2 ke 180 derajat");
  delay(1000);
}

//linear aktuator aja
const int ENA_PIN = 9;
const int IN1_PIN = 6;
const int IN2_PIN = 5;

void setup() {
  Serial.begin(115200); 
  pinMode(ENA_PIN, OUTPUT);
  pinMode(IN1_PIN, OUTPUT);
  pinMode(IN2_PIN, OUTPUT);
  digitalWrite(ENA_PIN, HIGH); 
}

void loop(){
  digitalWrite(IN1_PIN, HIGH);
  digitalWrite(IN2_PIN, LOW);
  delay(3000);
  digitalWrite(IN1_PIN, LOW);
  digitalWrite(IN2_PIN, LOW);
  delay(2000);

  digitalWrite(IN1_PIN, LOW);
  digitalWrite(IN2_PIN, HIGH);
  delay(3000);
  digitalWrite(IN1_PIN, LOW);
  digitalWrite(IN2_PIN, LOW);
  delay(2000);

}

